+		 NIX_RX_OFFLOAD_VLAN_STRIP_F | NIX_RX_OFFLOAD_SECURITY_F | NIX_RX_MULTI_SEG_F);
+	struct cn9k_sso_hws_dual *dws = port;
+	uint16_t ret = 1;
