--- a/arch/arm/dts/cn9130-cf-base.dts
+++ b/arch/arm/dts/cn9130-cf-base.dts
@@ -140,18 +140,18 @@
 	};
 };
 
-&cp0_pinctrl {
+&cp0_pinctl {
 	pinctrl-0 = <&sim_select_pins>;
 	pintrl-names = "default";
 
 	rear_button_pins: cp0-rear-button-pins {
-		marvell,pins = "mpp31";
-		marvell,function = "gpio";
+		marvell,pins = <31>;
+		marvell,function = <0>;
 	};
 
 	sim_select_pins: cp0-sim-select-pins {
-		marvell,pins = "mpp27";
-		marvell,function = "gpio";
+		marvell,pins = <27>;
+		marvell,function = <0>;
 	};
 };
 
